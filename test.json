
CREATE TABLE v4_sqq_mkt_new.mall_subject_join_record (
`id` int(11) NOT NULL AUTO_INCREMENT,
`group_id` int(11) NOT NULL,
`tenant_id` int(11) NOT NULL,
`activity_id` varchar(32) NOT NULL COMMENT '活动ID',
`commodity_id` varchar(64) NOT NULL COMMENT '商品ID 卡券：couponId',
`commodity_name` varchar(128) NOT NULL COMMENT '券名称',
`commodity_num` int(11) NOT NULL COMMENT '数量',
`member_id` varchar(128) DEFAULT NULL COMMENT '会员id',
`head_pic` varchar(256) DEFAULT NULL COMMENT '用户头像',
`nike_name` varchar(64) DEFAULT NULL COMMENT '昵称',
`mobile` varchar(16) DEFAULT NULL COMMENT '会员手机号',
`grade_code` varchar(50) DEFAULT NULL COMMENT '会员等级',
`join_status` tinyint(2) DEFAULT NULL COMMENT '报名状态 1 待审核 2 成功  3 失败 4 取消',
`join_fail_reason` varchar(500) DEFAULT NULL COMMENT '参与失败原因',
`position` varchar(128) DEFAULT NULL COMMENT '位置',
`remark` varchar(512) DEFAULT NULL COMMENT '报名备注',
`order_no` varchar(64) DEFAULT NULL COMMENT '订单号',
`order_status` int(2) NOT NULL DEFAULT '1' COMMENT '订单状态 1 待支付 2 支付  3 已关闭',
`send_time` datetime DEFAULT NULL COMMENT '发券时间',
`send_status` smallint(6) DEFAULT NULL COMMENT '发券状态 0 未发放 1 已发放 2 发放失败',
`fail_reason` varchar(128) DEFAULT NULL COMMENT '发放失败原因',
`refund_status` smallint(6) DEFAULT NULL COMMENT '退款状态 0 未退款,1 已退款',
`refund_flow_no` varchar(20) COMMENT '退款流水号',
`refund_fail_reason` varchar(128) COMMENT '退款失败原因',
`client_type` smallint(6) NOT NULL DEFAULT '1' COMMENT '客户端类型 0 普通浏览器,1 微信h5,2 支付宝H5,3 微信小程序',
`buy_point` int(11) DEFAULT NULL COMMENT '积分值',
`flow_no` varchar(20) DEFAULT NULL COMMENT '流水号',
`coupon_id` varchar(128) DEFAULT NULL COMMENT '卡券ID',
`create_time` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP,
`update_time` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=38 DEFAULT CHARSET=utf8mb4 COMMENT='商城报名记录'